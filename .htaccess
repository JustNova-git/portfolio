<ifModule mod_expires.c>
 ExpiresActive On
 #кэшировать флэш и изображения на одну неделю
 ExpiresByType image/x-icon "access plus 24 days"
 ExpiresByType image/jpeg "access plus 247 days"
 ExpiresByType image/png "access plus 24 days"
 ExpiresByType image/gif "access plus 24 days"
  ExpiresByType image/webp "access plus 24 days"
 ExpiresByType application/x-shockwave-flash "access plus 24 days"
 #кэшировать css, javascript и текстовые файлы на одну неделю
 ExpiresByType text/css "access plus 24 days"
 ExpiresByType text/javascript "access plus 24 days"
 ExpiresByType application/javascript "access plus 24 days"
 ExpiresByType application/x-javascript "access plus 24 days"
 #кэшировать html и htm файлы на один день
 ExpiresByType text/html "access plus 24 day"
 #кэшировать xml файлы на десять минут
 ExpiresByType application/xhtml+xml "access plus 60 minutes"
</ifModule>